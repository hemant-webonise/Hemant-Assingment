
public class Restorants {
	public Restorants(int restorantId, int price, String food) {
		super();
		RestorantId = restorantId;
		Price = price;
		Food = food;
	}
	int RestorantId;
	int Price;
	String Food;
	
	public int getRestorantId() {
		return RestorantId;
	}
	public void setRestorantId(int restorantId) {
		RestorantId = restorantId;
	}
	public int getPrice() {
		return Price;
	}
	public void setPrice(int price) {
		Price = price;
	}
	public String getFood() {
		return Food;
	}
	public void setFood(String food) {
		Food = food;
	}
	

}
